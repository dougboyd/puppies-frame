<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dougboyd.dogframe</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>-u</string>
        <string>server.py</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/doug/dogframe</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/doug/dogframe/logs/server.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/doug/dogframe/logs/server.error.log</string>
</dict>
</plist>
